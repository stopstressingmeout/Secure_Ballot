generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model VoterInput {
  NID    String   @id
  name   String
  father String
  mother String
  dob    DateTime
}

model Voter {
  NID     String   @id
  name    String
  father  String
  mother  String
  dob     DateTime
  phone   String
  email   String?
  address String?
}